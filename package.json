{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pastable-clipboard-history",
  "title": "Pastable Clipboard History",
  "description": "Instantly paste clipboard items from positions 0-9 with dedicated keyboard shortcuts",
  "icon": "clipboard",
  "author": "wook",
  "categories": ["Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "paste-current",
      "title": "Paste Current Clipboard Item",
      "description": "Paste the current clipboard item (position 0)",
      "mode": "no-view"
    },
    {
      "name": "paste-first",
      "title": "Paste 1st Previous Clipboard Item",
      "description": "Paste the 1st previous clipboard item (offset 1)",
      "mode": "no-view"
    },
    {
      "name": "paste-second",
      "title": "Paste 2nd Previous Clipboard Item",
      "description": "Paste the 2nd previous clipboard item (offset 2)",
      "mode": "no-view"
    },
    {
      "name": "paste-third",
      "title": "Paste 3rd Previous Clipboard Item",
      "description": "Paste the 3rd previous clipboard item (offset 3)",
      "mode": "no-view"
    },
    {
      "name": "paste-fourth",
      "title": "Paste 4th Previous Clipboard Item",
      "description": "Paste the 4th previous clipboard item (offset 4)",
      "mode": "no-view"
    },
    {
      "name": "paste-fifth",
      "title": "Paste 5th Previous Clipboard Item",
      "description": "Paste the 5th previous clipboard item (offset 5)",
      "mode": "no-view"
    },
    {
      "name": "paste-sixth",
      "title": "Paste 6th Previous Clipboard Item",
      "description": "Paste the 6th previous clipboard item (offset 6)",
      "mode": "no-view"
    },
    {
      "name": "paste-seventh",
      "title": "Paste 7th Previous Clipboard Item",
      "description": "Paste the 7th previous clipboard item (offset 7)",
      "mode": "no-view"
    },
    {
      "name": "paste-eighth",
      "title": "Paste 8th Previous Clipboard Item",
      "description": "Paste the 8th previous clipboard item (offset 8)",
      "mode": "no-view"
    },
    {
      "name": "paste-ninth",
      "title": "Paste 9th Previous Clipboard Item",
      "description": "Paste the 9th previous clipboard item (offset 9)",
      "mode": "no-view"
    },
    {
      "name": "pop-current",
      "title": "Pop Current Clipboard Item",
      "description": "Paste and remove the current clipboard item (offset 0)",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.101.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
} 